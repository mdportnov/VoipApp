<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/caller"
    app:startDestination="@id/navigation_caller">
    <fragment
        android:id="@+id/navigation_caller"
        android:name="ru.mephi.voip.ui.caller.CallerFragment"
        android:label="@string/title_calls"
        tools:layout="@layout/fragment_caller">
        <argument
            android:name="caller_name"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="caller_number"
            android:defaultValue="@null"
            app:nullable="true"
            app:argType="string" />
    </fragment>
</navigation>